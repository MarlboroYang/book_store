<h1>會員登入</h1>



<div>
<div>
<%= form_with(model: @user, url: sign_in_users_path, method: 'post', local: true) do |f| %>
<div>
<td><%= f.label :email %></td>
<td><%= f.text_field :email %></td>
<td><%= f.label :password, "密碼" %></td>
<td><%= f.password_field :password %></td>
<td><%= f.submit "登入"%></td>
</div>
<% end %>
</div>
</div>
